{
  "name": "narno/phpoole",
  "description": "PHPoole is a light and easy static website generator written in PHP.",
  "type": "project",
  "keywords": ["website", "static", "generator"],
  "license": "MIT",
  "homepage": "https://github.com/Narno/PHPoole/",
  "authors": [
    {
      "name": "Arnaud Ligny",
      "email": "arnaud@ligny.org",
      "homepage": "http://narno.org",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.4",
    "michelf/php-markdown": "1.*",
    "twig/twig": "1.*",
    "zendframework/zend-console": "2.*",
    "zendframework/zend-eventmanager": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*"
  },
  "bin": ["phpoole.php"],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
