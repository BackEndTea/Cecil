{
    "chmod": "0755",
    "directories": [
        "config",
        "plugins",
        "src"
    ],
    "files": [
        "LICENSE"
    ],
    "finder": [
        {
            "name": [
                "LICENSE*",
                "License*",
                "*.php",
                "*.xml",
                "*.xsd",
                "*.yaml",
                "*.yml",
                "*.json"
            ],
            "notName": [
                "composer.json"
            ],
            "exclude": [
                "phpunit",
                "Tests",
                "tests"
            ],
            "in": [
                "vendor"
            ]
        }
    ],
    "git-version": "git_version",
    "main": "bin/phpoole.php",
    "output": "dist/phpoole.phar",
    "stub": true
}